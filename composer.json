            "Ottosmops\\Antondate\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ottosmops\\Antondate\\AntondateServiceProvider"
            ],
            "aliases": {
                "Antondate": "Ottosmops\\Antondate\\Facades\\Antondate"
            }
        }
    }
