{
    "name": "ottosmops/antondate",
    "description": "Date which are used for Anton",
    "license": "license",
    "authors": [
        {
            "name": "ak",
            "email": "kraenzle@k-r.ch",
            "homepage": "https://www.k-r.ch"
        }
    ],
    "homepage": "https://github.com/ottosmops/antondate",
    "keywords": ["Laravel", "Antondate"],
    "require": {
        "laravel/framework": "~7|~8",
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
